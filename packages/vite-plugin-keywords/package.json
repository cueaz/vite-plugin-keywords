{
  "name": "vite-plugin-keywords",
  "version": "1.0.0",
  "description": "A Vite plugin that provides minifiable Symbols (keywords) to use in place of string literals for aggressive minification.",
  "keywords": [
    "vite",
    "vite-plugin",
    "minification"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cueaz/vite-plugin-keywords.git"
  },
  "license": "MIT",
  "author": "cueaz",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "keywords": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest"
  },
  "dependencies": {
    "minifiable-keywords": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0"
  }
}
